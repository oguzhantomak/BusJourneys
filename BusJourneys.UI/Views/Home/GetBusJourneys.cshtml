@model List<BusJourneys.Core.Models.Responses.GetBusJourneysResponseDto.DataDto>
@{
    ViewData["Title"] = "GetBusJourneys";
}

<ul class="list-group">
    @foreach (var item in Model)
    {
        <li class="list-group-item d-flex justify-content-between align-items-center">
            #@item.Journey.Departure# @item.Journey.Origin - @item.Journey.Destination
            <span class="badge bg-primary rounded-pill">@item.Journey.InternetPrice @item.Journey.Currency</span>
        </li>
    }
</ul>

